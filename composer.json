{
    "name": "tobento/app-language",
    "type": "library",
    "description": "App language support.",
    "keywords": [
        "tobento",
        "package",
        "app",
        "language"
    ],
    "homepage": "https://www.tobento.ch",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Strub",
            "email": "info@tobento.ch",
            "homepage": "https://www.tobento.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "tobento/app": "^1.0",
        "tobento/app-migration": "^1.0",
        "tobento/app-http": "^1.0.6",
        "tobento/app-database": "^1.0",
        "tobento/service-support": "^1.0",
        "tobento/service-dater": "^1.0.2",
        "tobento/service-routing": "^1.0.8",
        "tobento/service-requester": "^1.0.2",
        "tobento/service-language": "^1.0.4",
        "tobento/service-repository": "^1.0",
        "tobento/service-repository-storage": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.0",
        "tobento/app-translation": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tobento\\App\\Language\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tobento\\App\\Language\\Test\\": "tests",
            "Tobento\\App\\Http\\Test\\": "vendor/tobento/app-http/tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --colors=always",
        "psalm": "vendor/bin/psalm"
    }
}